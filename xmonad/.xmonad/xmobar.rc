Config
  { font          = "xft:monofur:size=11"
  , fgColor       = "#ffffff"
  , bgColor       = "#808080"
  , position      = TopP 24 24
  , border        = NoBorder
  , lowerOnStart  = True
  , commands      =
    [ Run StdinReader
    , Run Date "%a %h %d %r" "date" 10
    ]
  , sepChar   = "%"
  , alignSep  = "}{"
  , template  = "%StdinReader% }{ %date% "
  }
