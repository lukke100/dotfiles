URxvt.scrollBar:            false
URxvt.font:                 xft:Terminess Powerline:size=9
URxvt.depth:                32
URxvt.saveLines:            0
URxvt.perl-ext:             default,url-select
URxvt.keysym.C-i:           perl:url-select:select_next
URxvt.url-select.launcher:  firefox-developer
URxvt.url-select.underline: true
URxvt.cursorUnderline:      true
URxvt.cursorBlink:          true

URxvt*foreground: rgba:db00/db00/db00/ffff
URxvt*background: rgba:4925/4925/4925/e000

Xft.antialias: true
Xft.autohint:  true
Xft.dpi:       96
Xft.hinting:   true
Xft.hintstyle: hintsfull
Xft.rgba:      rgb

rofi.modi:              run,window,ssh
rofi.opacity:           100
rofi.width:             50
rofi.lines:             15
rofi.columns:           1
rofi.font:              Terminus 9
rofi.color-normal:      #404040,#dbdbdb,#404040,#7c60bf,#ececec
rofi.color-urgent:      #404040,#dc322f,#404040,#dc322f,#ececec
rofi.color-active:      #404040,#dbdbdb,#404040,#7c60bf,#ececec
rofi.color-window:      #404040,#7c60bf,#dbdbdb
rofi.bw:                2
rofi.location:          2
rofi.padding:           2
rofi.yoffset:           66
rofi.xoffset:           0
rofi.fixed-num-lines:   false
rofi.terminal:          urxvt
rofi.ssh-client:        ssh
rofi.disable-history:   true
rofi.levenshtein-sort:  true
rofi.case-sensitive:    false
rofi.sidebar-mode:      false
rofi.eh:                1
rofi.line-margin:       2
rofi.separator-style:   none
rofi.hide-scrollbar:    true
rofi.fake-transparency: false
rofi.threads:           4
rofi.scrollbar-width:   8
rofi.scroll-method:     1
rofi.fake-background:   screenshot
rofi.window-format:     {w}   {c}   {t}
rofi.kb-cancel:         Escape,Control+bracketleft
