URxvt.scrollBar:            false
URxvt.font:                 xft:Terminess Powerline:size=9
URxvt.depth:                32
URxvt.saveLines:            0
URxvt.perl-ext:             default,url-select
URxvt.keysym.C-i:           perl:url-select:select_next
URxvt.url-select.launcher:  firefox-nightly
URxvt.url-select.underline: true
URxvt.cursorUnderline:      true
URxvt.cursorBlink:          true

URxvt*foreground:       #ffffff
URxvt*background:   [66]#2f163f

! ! Black + DarkGrey
! URxvt*color0:           #000000
! URxvt*color8:           #242424
! ! DarkRed + Red
! URxvt*color1:           #b08080
! URxvt*color9:           #e0b0b0
! ! DarkGreen + Green
! URxvt*color2:           #80b080
! URxvt*color10:          #b0e0b0
! ! DarkYellow + Yellow
! URxvt*color3:           #b0b080
! URxvt*color11:          #e0e0b0
! ! DarkBlue + Blue
! URxvt*color4:           #8080b0
! URxvt*color12:          #b0b0e0
! ! DarkMagenta + Magenta
! URxvt*color5:           #b080b0
! URxvt*color13:          #e0b0e0
! !DarkCyan + Cyan
! URxvt*color6:           #80b0b0
! URxvt*color14:          #b0e0e0
! ! LightGrey + White
! URxvt*color7:           #b0a992
! URxvt*color15:          #ffffff

Xft.antialias:          true
Xft.autohint:           true
Xft.dpi:                96
Xft.hinting:            true
Xft.hintstyle:          hintsfull
Xft.rgba:               rgb

! Rofi config
rofi.switchers:         window,run,ssh
rofi.opacity:           100
rofi.width:             640
rofi.lines:             5
rofi.columns:           1
rofi.font:              Terminus (TTF) 9
rofi.fg:                #ffffff
rofi.bg:                #2f163f
rofi.hlfg:              #2f163f
rofi.hlbg:              #cfbfdf
rofi.bc:                #cfbfdf
rofi.bw:                0
rofi.location:          2
rofi.padding:           0
rofi.yoffset:           40
rofi.xoffset:           0
rofi.fixed-num-lines:   false
rofi.hmode:             false
rofi.terminal:          urxvt
rofi.ssh-client:        ssh
rofi.ssh-command:       {terminal} -e {ssh-client} {host}
rofi.run-command:       {cmd}
rofi.run-list-command:
rofi.run-shell-command: {terminal} -e {cmd}
rofi.disable-history:   true
rofi.levenshtein-sort:  false
rofi.case-sensitive:    false
rofi.key-window:        mod1+o
rofi.key-run:           mod1+p
rofi.key-ssh:           mod1+i
rofi.sidebar-mode:      false
rofi.lazy-filter-limit: 5000

