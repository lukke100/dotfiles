URxvt.scrollBar:            false
URxvt.font:                 xft:Terminess Powerline:size=9
URxvt.depth:                32
URxvt.saveLines:            0
URxvt.perl-ext:             default,url-select
URxvt.keysym.C-i:           perl:url-select:select_next
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt*cursorUnderline:      false

URxvt*foreground:   #ffffff
URxvt*background:   [75]#000000

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintsfull
Xft.autohint:   true

! Black + DarkGrey
URxvt*color0:   #532621
URxvt*color8:   #6c1714
! DarkRed + Red
URxvt*color1:   #723437
URxvt*color9:   #b83032
! DarkGreen + Green
URxvt*color2:   #bc8080
URxvt*color10:  #aa1e1f
! DarkYellow + Yellow
URxvt*color3:   #eeb2b2
URxvt*color11:  #d57372
! DarkBlue + Blue
URxvt*color4:   #904a52
URxvt*color12:  #bc4e4f
! DarkMagenta + Magenta
URxvt*color5:   #a76569
URxvt*color13:  #d01617
!DarkCyan + Cyan
URxvt*color6:   #d29091
URxvt*color14:  #93110f
! LightGrey + White
URxvt*color7:   #862627
URxvt*color15:  #f22021

! Rofi config
rofi.switchers:         window,run,ssh
rofi.opacity:           100
rofi.width:             1920
rofi.lines:             65
rofi.columns:           1
rofi.font:              terminus 9
rofi.fg:                #ffffff
rofi.bg:                argb:C0000000
! rofi.bgalt:             #532621
rofi.hlfg:              #aa1e1f
rofi.hlbg:              #ffffff
rofi.bc:                #ffffff
rofi.bw:                0
rofi.location:          2
rofi.padding:           13
rofi.yoffset:           -3
rofi.xoffset:           0
rofi.fixed-num-lines:   false
rofi.hmode:             false
rofi.terminal:          urxvt
rofi.ssh-client:        ssh
rofi.ssh-command:       {terminal} -e {ssh-client} {host}
rofi.run-command:       {cmd}
rofi.run-list-command:
rofi.run-shell-command: {terminal} -e {cmd}
rofi.disable-history:   false
rofi.levenshtein-sort:  false
rofi.case-sensitive:    false
rofi.key-window:        mod1+o
rofi.key-run:           mod1+p
rofi.key-ssh:           mod1+i
rofi.sidebar-mode:      false
rofi.lazy-filter-limit: 5000

