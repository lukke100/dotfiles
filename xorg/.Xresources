URxvt.scrollBar:            false
URxvt.font:                 xft:Terminess Powerline:size=9
URxvt.depth:                32
URxvt.saveLines:            0
URxvt.perl-ext:             default,url-select
URxvt.keysym.C-i:           perl:url-select:select_next
URxvt.url-select.launcher:  firefox-nightly
URxvt.url-select.underline: true
URxvt.cursorUnderline:      true
URxvt.cursorBlink:          true

URxvt*foreground:      #d0cdd7
URxvt*background: [100]#35313b

! Black
*color0:  #35313b
*color8:  #4f4c56

! Red
*color1:  #885163
*color9:  #b98898

! Green
*color2:  #5c845b
*color10: #92b692

! Yellow
*color3:  #887d5b
*color11: #b9af92

! Blue
*color4:  #55598e
*color12: #8c8ec0

! Magenta
*color5:  #81518e
*color13: #b388c0

! Cyan
*color6:  #558487
*color14: #8cb6b9

! White
*color7:  #aeaab4
*color15: #d0cdd7

Xft.antialias:          true
Xft.autohint:           true
Xft.dpi:                96
Xft.hinting:            true
Xft.hintstyle:          hintsfull
Xft.rgba:               rgb

