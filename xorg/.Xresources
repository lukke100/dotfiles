URxvt.scrollBar:            false
URxvt.font:                 xft:monospace:size=9
URxvt.depth:                32
URxvt.saveLines:            0
URxvt.perl-ext:             default,url-select
URxvt.keysym.C-i:           perl:url-select:select_next
URxvt.url-select.launcher:  google-chrome-stable
URxvt.url-select.underline: true
URxvt.cursorUnderline:      true
URxvt.cursorBlink:          true

URxvt*foreground: rgba:0000/0000/0000/ffff
URxvt*background: rgba:ffff/ffff/ffff/cfff

URxvt*color0:  #000000
URxvt*color1:  #7e1741
URxvt*color2:  #1b511d
URxvt*color3:  #6d2717
URxvt*color4:  #511da3
URxvt*color5:  #6b1970
URxvt*color6:  #443879
URxvt*color7:  #bfbfbf

URxvt*color8:  #7f7f7f
URxvt*color9:  #e65e62
URxvt*color10: #569d1c
URxvt*color11: #c27a2c
URxvt*color12: #7d81db
URxvt*color13: #d35aca
URxvt*color14: #66964f
URxvt*color15: #ffffff

Xft.antialias: true
Xft.autohint:  true
Xft.dpi:       96
Xft.hinting:   true
Xft.hintstyle: hintsfull
Xft.rgba:      rgb

rofi.modi:              run,window,ssh
rofi.opacity:           100
rofi.width:             50
rofi.lines:             15
rofi.columns:           1
rofi.font:              monospace 10
rofi.color-normal:      #ffffff,#000000,#ffffff,#d79fcf,#000000
rofi.color-urgent:      #ffffff,#dc322f,#ffffff,#dc322f,#ececec
rofi.color-active:      #ffffff,#000000,#ffffff,#d79fcf,#000000
rofi.color-window:      #ffffff,#000000,#000000
rofi.bw:                1
rofi.location:          2
rofi.padding:           1
rofi.yoffset:           51
rofi.xoffset:           0
rofi.fixed-num-lines:   false
rofi.terminal:          urxvt
rofi.ssh-client:        ssh
rofi.disable-history:   true
rofi.levenshtein-sort:  true
rofi.case-sensitive:    false
rofi.sidebar-mode:      false
rofi.eh:                1
rofi.line-margin:       2
rofi.separator-style:   none
rofi.hide-scrollbar:    true
rofi.fake-transparency: false
rofi.threads:           4
rofi.scrollbar-width:   8
rofi.scroll-method:     1
rofi.fake-background:   screenshot
rofi.window-format:     {w}   {c}   {t}
rofi.kb-cancel:         Escape,Control+bracketleft
